<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingTop="12dp"
    android:paddingLeft="20dp"
    android:paddingRight="20dp"
    android:paddingBottom="6dp"
    android:background="@color/white"
    android:layout_marginTop="10dp">


    <CheckBox
        android:id="@+id/cb_multi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:button="@drawable/selector_checkbox_multi"
        android:checked="false"
        android:clickable="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        />

    <RelativeLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="5dp"
        android:paddingLeft="20dp"
        android:paddingBottom="5dp">

        <TextView
            android:id="@+id/item_tv_clear_com_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:text="tv_eventid"/>


        <TextView
            android:id="@+id/item_tv_clear_com_no"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/item_tv_clear_com_name"
            android:textSize="12sp"
            android:text="测试一"/>



        <TextView
            android:id="@+id/item_tv_clear_task_status"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="14sp"
            android:layout_alignParentRight="true"
            android:text="完成"/>

        <TextView
            android:id="@+id/item_tv_clear_end_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:layout_below="@id/item_tv_clear_task_status"
            android:layout_alignParentRight="true"
            android:layout_marginTop="5dp"
            android:text="截止日期：2015-07-12 20:34:34"/>

    </RelativeLayout>

</RelativeLayout>